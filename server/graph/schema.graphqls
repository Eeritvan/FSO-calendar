scalar Time
scalar UUID

type CalEvent {
  id: UUID!
  name: String!
  description: String
  startTime: Time!
  endTime: Time!
}

input CalEventInput {
  name: String!
  description: String
  startTime: Time!
  endTime: Time!
}

type Query {
  allEvents: [CalEvent!]
}

type Mutation {
  createCalEvent(input: CalEventInput!): CalEvent!
  deleteCalEvent(input: UUID!): Boolean!
}
