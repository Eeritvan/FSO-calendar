scalar Time

type CalEvent {
  name: String!
  description: String
  start_time: Time!
  end_time: Time!
}

input NewCalEvent {
  name: String!
  description: String
  start_time: Time!
  end_time: Time!
}

type Query {
  getAllEvents: [CalEvent!]
}

type Mutation {
  createCalEvent(input: NewCalEvent!): CalEvent!
}
